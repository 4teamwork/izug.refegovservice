<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="izug.refegovservice">

<body>
<div metal:fill-slot="main" class="webContent">
  <tal:con tal:condition="context/getReferencedService">
    <!-- redefine context to referenced object to display the referenced object -->
    <tal:block tal:define="context context/getReferencedService">
      <metal:use_body use-macro="context/@@izug_refegovsercie_macros/leistung" />
    </tal:block>
  </tal:con>
  <tal:con tal:condition="not: context/getReferencedService">
    <h1 i18n:translate="">Please upgrade this object to the new Egov Leistung.</h1>
  </tal:con>
</div>
</body>
</html>
